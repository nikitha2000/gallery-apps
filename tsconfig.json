{ 
    "compilerOptions": { 
        "target": "es5", 
        "module": "commonjs", 
        "jsx": "react-jsx", 
        "strict": true, 
        "esModuleInterop": true, 
        "skipLibCheck": true, 
        "forceConsistentCasingInFileNames": true, 
        "moduleResolution": "node", 
        "resolveJsonModule": true, 
        "isolatedModules": true, 
        "noEmit": true 
        }, 
        "include": ["src"] 
    }